{
  "version": "0.2.1",
  "npmClient": "yarn",
  "useWorkspaces": true,
  "registry": "https://registry.npmjs.org",
  "packages": [
    "packages/*"
  ],
  "command": {
    "publish": {
      "allowBranch": [
        "dev",
        "master"
      ],
      "message": "chore(release): publish %s"
    }
  }
}
